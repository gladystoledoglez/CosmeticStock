<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/pink_alpha_100</item>
        <item name="colorPrimaryDark">@color/pink_300</item>
        <item name="colorAccent">@color/pink_alpha_400</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:navigationBarColor">@color/pink_alpha_100</item>
    </style>

    <style name="AppTheme.Fullscreen" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/pink_200</item>
        <item name="colorPrimaryDark">@color/pink_200</item>
        <item name="android:windowBackground">@color/pink_200</item>
        <item name="android:navigationBarColor">@color/pink_200</item>
    </style>
</resources>